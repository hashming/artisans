<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.duoduo.hashming.artisan.dao.UserMapper" >

    <sql id="BASE_TABLE">
        user
    </sql>

    <sql id="BASE_COLUMN">
        id,account_id,name,token,gmt_create,gmt_modified
    </sql>

    <insert id="insert" parameterType="com.duoduo.hashming.artisan.model.User">
        INSERT INTO
        <include refid="BASE_TABLE"/>
        <trim prefix="(" suffix=")" suffixOverrides=",">
            account_id,name,token,gmt_create,gmt_modified,
        </trim>
        <trim prefix="VALUES(" suffix=")" suffixOverrides=",">
            #{account_id, jdbcType=VARCHAR},#{name, jdbcType=VARCHAR},#{token, jdbcType=VARCHAR},#{gmt_create, jdbcType=BIGINT},#{gmt_modified, jdbcType=BIGINT},
        </trim>
    </insert>

   <!-- <select id="selectUsers" resultType="com.duoduo.springbootjdbc.springboot_jdbc.pojo.User">
        SELECT
        <include refid="BASE_COLUMN"/>
        FROM
        <include refid="BASE_TABLE"/>
    </select>-->


</mapper>